@model IEnumerable<RoxierMongoDB.Models.Maquina>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.recvTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.attrName)
        </th>
        <th hidden>
            @Html.DisplayNameFor(model => model.attrType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.attrValue)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.recvTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.attrName)
        </td>
        <td hidden>
            @Html.DisplayFor(modelItem => item.attrType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.attrValue)
        </td>
    </tr>
}

</table>
